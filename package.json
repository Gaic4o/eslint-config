{
  "name": "@feature-sliced/eslint-config",
  "version": "0.1.0-beta.6",
  "description": "🍰 Lint feature-sliced concepts by existing eslint plugins",
  "main": "index.js",
  "files": [
    "rules/**/experimental.js",
    "rules/**/lite.js",
    "rules/**/index.js",
    "utils/**",
    "index.js"
  ],
  "repository": "https://github.com/feature-sliced/eslint-config.git",
  "author": "FeatureSliced core-team<feature.sliced@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "feature-sliced",
    "feature-slices",
    "feature-driven",
    "feature-based"
  ],
  "scripts": {
    "publish:prepatch": "npm version prepatch && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "prettier:fix": "prettier --write **/*.js",
    "cli:run": "node cli/cli.js",
    "cli:dev": "nodemon cli/cli.js",
    "clean": "git clean -fxd",
    "test": "mocha \"*(test|rules)/**/*.test.js\""
  },
  "peerDependencies": {
    "eslint-plugin-boundaries": "2.8.0",
    "eslint-plugin-import": "2.25.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint-kit/eslint-config-base": "^5.0.2",
    "@eslint-kit/eslint-config-patch": "^1.0.0",
    "@eslint-kit/eslint-config-prettier": "^4.0.0",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "7.10.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-boundaries": "2.8.0",
    "eslint-plugin-import": "2.25.4",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.15",
    "prettier": "2.3.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "meow": "^8.1.2",
    "picocolors": "^1.0.0"
  }
}
